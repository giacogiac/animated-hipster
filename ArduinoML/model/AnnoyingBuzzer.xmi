<?xml version="1.0" encoding="ASCII"?>
<arduinoml:Machine
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:arduinoml="http://arduinoml/1.0"
    start="off">
  <bricks xsi:type="arduinoml:Actuator"
      name="buzzer"
      pin="10"/>
  <states name="off">
    <transitions
        goto="on">
      <conditions
          xsi:type="arduinoml:TimeCondition"
          time="1500"/>
    </transitions>
    <actions
        actuator="buzzer"/>
  </states>
  <states name="on">
    <transitions
        goto="beforeOff">
      <conditions
          xsi:type="arduinoml:TimeCondition"
          time="100"/>
    </transitions>
    <actions
        bState="ON"
        actuator="buzzer"/>
  </states>
  <states name="beforeOff">
    <transitions
        goto="beforeOn">
      <conditions
          xsi:type="arduinoml:TimeCondition"
          time="100"/>
    </transitions>
    <actions
        actuator="buzzer"/>
  </states>
  <states name="beforeOn">
    <transitions
        goto="off">
      <conditions
          xsi:type="arduinoml:TimeCondition"
          time="100"/>
    </transitions>
    <actions
        bState="ON"
        actuator="buzzer"/>
  </states>
</arduinoml:Machine>
